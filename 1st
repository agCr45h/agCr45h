import pygame
pygame.init()
screen=pygame.display.set_mode([640,480])
screen.fill([255,255,255])
avto=pygame.image.load("avto.png")
screen.blit(avto,[50,50])([50,50]
pygame.display.flip()
while running:
  for event in pygame.event.get():
    if event.type==pygame.QUIT:
       running=False
pygame.quit()
